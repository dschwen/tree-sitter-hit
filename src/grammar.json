    "parameter_name": {
      "type": "SYMBOL",
      "name": "_path"
    },
    "parameter_value": {
      "type": "SYMBOL",
      "name": "_string"
    },
    "_string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^\\s\\'\\\"]+"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "IMMEDIATE_TOKEN",
                    "content": {
                      "type": "PREC",
                      "value": 2,
                      "content": {
                        "type": "PATTERN",
                        "value": "[^\\']+"
                      }
                    }
                  },
                  {
                    "type": "SYMBOL",
                    "name": "escape_sequence"
                  }
                ]
              }
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "IMMEDIATE_TOKEN",
                    "content": {
                      "type": "PREC",
                      "value": 2,
                      "content": {
                        "type": "PATTERN",
                        "value": "[^\\\"]+"
                      }
                    }
                  },
                  {
                    "type": "SYMBOL",
                    "name": "escape_sequence"
                  }
                ]
              }
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        }
      ]
